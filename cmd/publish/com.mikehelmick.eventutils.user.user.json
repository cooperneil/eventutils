{
  "$schema": "http://json-schema.org/schema#",
  "$ref": "https://schemas.in-the-cloud.dev/download/com.mikehelmick.eventutils.user.user.json",
  "required": [
    "first_name",
    "last_name",
    "email"
  ],
  "properties": {
    "age": {
      "type": "integer"
    },
    "city": {
      "type": "string"
    },
    "country": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "first_name": {
      "type": "string"
    },
    "last_name": {
      "type": "string"
    }
  },
  "additionalProperties": false,
  "type": "object"
}